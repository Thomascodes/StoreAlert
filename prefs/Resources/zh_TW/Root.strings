<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Label for switch to enable or disable the tweak. -->
	<key>ENABLED</key>
	<string>啟用</string>

	<!-- Label for switch to enable or disable displaying the URL in the alert. -->
	<key>SHOW_URL</key>
	<string>在警告中顯示 URL</string>

	<!-- Button leading to a list of apps to enable or disable showing the alert within. -->
	<key>DISPLAY_IN</key>
	<string>顯示在</string>

	<!-- Explanation of the above feature. -->
	<key>DISPLAY_IN_EXPLANATION</key>
	<string>對信任的 App 關閉 StoreAlert，或者在 App 商店顯示確認。</string>
</dict>
</plist>
