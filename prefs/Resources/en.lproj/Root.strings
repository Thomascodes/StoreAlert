<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Label for switch to enable or disable the tweak. -->
	<key>ENABLED</key>
	<string>Enabled</string>

	<!-- Label for switch to enable or disable displaying the URL in the alert. -->
	<key>SHOW_URL</key>
	<string>Show URL in Alert</string>

	<!-- Button leading to a list of apps to enable or disable showing the alert within. -->
	<key>DISPLAY_IN</key>
	<string>Display In</string>

	<!-- Explanation of the above feature. -->
	<key>DISPLAY_IN_EXPLANATION</key>
	<string>Turn off StoreAlert for apps you trust, or that show their own App Store confirmation.</string>
</dict>
</plist>
